@model IEnumerable<SalesApp.Models.Entities.I_Sales>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FlightIn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FlightOut)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dates)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Comment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PrecioNett)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PrecioNettTotalPax)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.I_Agent.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.I_Itinerary.MeetingPoint)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.I_SalesGroup.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.I_Pax.First_Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FlightIn)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FlightOut)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dates)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Comment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PrecioNett)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PrecioNettTotalPax)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.I_Agent.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.I_Itinerary.MeetingPoint)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.I_SalesGroup.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.I_Pax.First_Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
